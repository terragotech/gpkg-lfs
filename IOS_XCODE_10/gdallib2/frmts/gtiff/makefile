	$(CXX) $(TIF_FLAGS) $(GEOTIF_FLAGS) $(PORT_INCLUDE) $(CORE_INCLUDE) $(OGR_INCLUDE) $(CXXFLAGS)  -c gt_citation.cpp
	$(CXX) $(TIF_FLAGS) $(GEOTIF_FLAGS) $(PORT_INCLUDE) $(CORE_INCLUDE) $(OGR_INCLUDE) $(CXXFLAGS)  -c gt_overview.cpp
	$(CC)  $(TIF_FLAGS) $(GEOTIF_FLAGS) $(PORT_INCLUDE) $(CORE_INCLUDE) $(OGR_INCLUDE) $(CFLAGS)  -c tif_float.c	 
	$(CXX) $(TIF_FLAGS) $(GEOTIF_FLAGS) $(PORT_INCLUDE) $(CORE_INCLUDE) $(OGR_INCLUDE) $(CXXFLAGS)  -c tifvsi.cpp	
	$(CXX) $(TIF_FLAGS) $(GEOTIF_FLAGS) $(PORT_INCLUDE) $(CORE_INCLUDE) $(OGR_INCLUDE) $(CXXFLAGS)  -c tifvsi.cpp	
	$(CXX) $(JPEG_LIBJPEG) $(FRMTS_INC) $(JPEG_INC) $(VRT_INCLUDE) $(TIF_FLAGS) $(GEOTIF_FLAGS) $(PORT_INCLUDE) $(CORE_INCLUDE) $(OGR_INCLUDE) $(CXXFLAGS)  -c gt_jpeg_copy.cpp

